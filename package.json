    "babel-preset-stage-2": "^6.17.0",
    "chai": "^3.5.0",
    "enzyme": "^2.4.1",
    "jsdom": "^9.6.0",
    "mocha": "^3.1.0",
    "react-addons-test-utils": "^15.3.2",
    "react-hot-loader": "^3.0.0-beta.5",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.1"
